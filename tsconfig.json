{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "react"
  },
  "include": ["src", "types"],
  "exclude": ["node_modules"]
}
